#include <stdio.h>
#include <stdlib.h>

int main()
{
    int two_dim[3][4]={ {0,1,2,3},
                        {1,3,5,7},
                        {3,4,6,8}
                        };
                        
    int i,j;
    for(i=0;i<3;i++){}
        for(j=0;j<4;j++)
            printf("%3d", two_dim[i][j]);
            printf("\n");
        
    }
    return 0;
}
