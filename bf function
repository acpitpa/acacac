#include <stdio.h>
#include <stdlib.h>
#define ROW_SIZE 3
#define COL_SIZE 3

int main()
{
    int i,j;
    int two_dim[ROW_SIZE][COL_SIZE];/*={  {0,1,2,3},
                                        {3,5},
                                        {2,4}
                                        };*/

    for(i=0;i<ROW_SIZE;i++) {
         for(j=0;j<COL_SIZE;j++){
         printf("Input [%d][%d]:", i,j);
         scanf("%3d",&two_dim[i][j]);
         }
    }

        for(i=0;i<ROW_SIZE;i++){
            for(j=0;j<COL_SIZE;j++){
            //printf("Input [%d][%d]:", i,j);
            printf("%3d",two_dim[i][j]);
            }
        printf("\n");
        }

    return 0;

}
